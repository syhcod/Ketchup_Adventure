--this is a heavy story game--
--made by eastern_magician--
function _init()
	palt(0, false)
	palt(14, true)
	interval=5
	x=30 y=30 f = false sp = 1 leg=33
	sec = 0
	hand=0
	legcheck = false
	smoke=0
	fp=0
	mx=100
	my=100
	
	wheel=39
	rob=23
	robf=false
	lser=false
	
	cola={
		zpush=0,
		xpush=0,
		mx=20,
		mxn=20,
		num=0,
		r=12,
		av=0.03,
		theta=0,
		group={}
	}
	
	laser={
		mxn=20,
		num=2,
		group={}
	}
	
	for i=1,cola.mxn do
			cola.group[i]={emp=true}
	end
	for i=1,laser.mxn do
			laser.group[i]={emp=true}
	end

end

function leg_change()
	if (sec%interval==0) then
		if (leg==33) then 
			leg = 35 legcheck=true
			return
		end
		if (leg==37) then 
			leg = 35 legcheck=false
			return
		end
		if (leg==35) then 
			if (legcheck) then
				leg=37
			else
				leg=33
			end
		end
	end
end

function update_smoke()
	if (sec%interval==0) then
		smoke+=1
		if wheel==39 then wheel=55
		else wheel=39 end
		if (smoke==3) then
			smoke=0
		end
	end
end

function throw_cola()
	if (cola.num>0) then
		ii=true
		ij=0
		while ii do
			ij=ij+1
			if ij>cola.mxn then
				ii=false
			else
				if cola.group[ij].emp==true then
					ii=false
				end
			end
		end
		if (ij<=cola.mxn) then
			cola.group[ij]={
				xx=x+5+cola.r*cos(cola.theta),
				yy=y+12+cola.r*sin(cola.theta)/2,
				vx=5*(1-2*fp),
				vy=0,
				dir=0,
				emp=false
			}
			cola.num=cola.num-1
			cola.theta=cola.theta+1/cola.mx
		end
	end
end

function throw_laser()
	if (laser.num>0) then
		ii=true
		ij=0
		while ii do
			ij=ij+1
			if ij>laser.mxn then
				ii=false
			else
				if laser.group[ij].emp==true then
					ii=false
				end
			end
		end
		if robf then rang=1
		else rang=0 end
		if (ij<=laser.mxn) then
			laser.group[ij]={
				xx=mx,
				yy=my,
				vx=5*(1-2*rang),
				vy=0,
				dir=0,
				emp=false
			}
			laser.num=laser.num-1
		end
	end
end

function _update()
	move=false
	sec=(sec+1)%256
	update_smoke()
	cola.theta=cola.theta+cola.av
	mv=0.6
	if (y-my)^2<10 then 
		rob=57
		if not lser then
			throw_laser()
			lser=true
		end
		if (x-mx)^2+(y-my)^2<10 then
			mv=0
		end
		if x>mx then
			mx=mx+mv
			robf=false
		else
			mx=mx-mv
			robf=true
		end
	else rob=23 end
	
	if (y-my)^2>10 then
		if y>my  then
			my=my+mv
		else
			my=my-mv
		end
	end

	--music--
	if (sec==1) then
		sfx(0,0)
		sfx(1,1)
	end
	if (sec==128) then
		sfx(0,0)
		sfx(2,2)
	end
	
--button press--
	if btn(üÖæÔ∏è) then
	 if  cola.num<cola.mx and cola.zpush==1 then
			cola.num=cola.num+1
			sfx(3,3)
		end
		cola.zpush=0
	else
			cola.zpush=1
	end
	if btn(‚ùé) then
		if cola.xpush==1 then
			throw_cola()
		end
		cola.xpush=0
	else
		cola.xpush=1
	end

	if (btn(‚¨ÜÔ∏è)) then 
		y-=1 
		move=true
		if (btn(‚¨áÔ∏è)) then
			move=false
		end
	end
	if (btn(‚¨áÔ∏è)) then 
		y+=1 
		move=true
		if (btn(‚¨ÜÔ∏è)) then
			move=false
		end
	end
	if (btn(‚û°Ô∏è)) then 
		if move==true then
			if (btn(‚¨ÖÔ∏è)) then
				leg_change()
				return
			end
		end
		move=true
		if (btn(‚¨ÖÔ∏è)) then
			move=false
			leg=33
			return
		end
		x+=1 
		f = false
		fp=0
		hand = 0
	end
	if (btn(‚¨ÖÔ∏è)) then 
		move=true
		x-=1
		f = true
		fp=1
		hand = 1
	end
	if (move) then leg_change()
	else leg=33
	end
	for i=1,cola.mxn do
		ci=cola.group[i]
		if ( ci.emp ==false) then
			ci.xx=ci.xx+ci.vx
			ci.yy=ci.yy+ci.vy
			ci.dir=(ci.dir+1)%4
		if (ci.xx<-10 or	ci.xx> 130)
					or 	(ci.yy<-10 or	ci.yy> 130)  then
					ci.emp=true	
			end
		end			  
	end
	for i=1,laser.mxn do
		ci=laser.group[i]
		if ( ci.emp ==false) then
			ci.xx=ci.xx+ci.vx
			ci.yy=ci.yy+ci.vy
			ci.dir=(ci.dir+1)%4
		if (ci.xx<-10 or	ci.xx> 130)
					or 	(ci.yy<-10 or	ci.yy> 130)  then
					ci.emp=true
					lser=false
					laser.num+=1
			end
		end			  
	end
end

function bl(x)
	if x==0 then
		return false
	else
		return true
	end
end

function _draw()
	cls(3)
	
	map(0,0)
	
	spr(leg, x, y+16, 2, 2, f, false)
	spr(16,x+8-(hand*11)-smoke*hand*2+smoke,y-smoke)
	if cola.num>0 then
		for i=0,cola.num-1 do
		
		 rot=cola.theta+1/cola.mx*i
		 
			spr(
				32,
				x+5+cola.r*cos(rot),
				y+12+cola.r*sin(rot)/2,
				1,
				1
			)
		end
	end
	for i=1,cola.mxn do
		if ( cola.group[i].emp ==false) then
		
			cd_short=cola.group[i].dir
			
			spr(
				32,
				cola.group[i].xx,
				cola.group[i].yy,
				1,
				1,
				bl(cd_short%2),
				bl((cd_short-cd_short%2)/2)
				)
		end
	end	
	for i=1,laser.mxn do
		if ( laser.group[i].emp ==false) then
		
			ld_short=laser.group[i].dir
			
			spr(
				9,
				laser.group[i].xx,
				laser.group[i].yy,
				1,
				1,
				bl(ld_short%2),
				bl((ld_short-ld_short%2)/2)
				)
		end
	end	
	spr(sp, x, y, 2, 2, f, false)
	spr(wheel,mx,my+8,2,1,robf,false)
	spr(rob,mx,my,2,1,robf,false)
end

