function _init()
	palt(0, false)
	palt(14, true)
	interval=5
	x=30 y=30 f = false sp = 1 leg=33
	sec = 0
	hand=0
	legcheck = false
	smoke=0
	fp=0
	
	cola={
		zpush=0,
		xpush=0,
		mx=8,
		mxn=20,
		num=0,
		r=20,
		av=0.03,
		theta=0,
		group={}
	}
	
	for i=1,cola.mxn do
			cola.group[i]=0
	end

	t=0

end

function leg_change()
	if (sec%interval==0) then
		if (leg==33) then 
			leg = 35 legcheck=true
			return
		end
		if (leg==37) then 
			leg = 35 legcheck=false
			return
		end
		if (leg==35) then 
			if (legcheck) then
				leg=37
			else
				leg=33
			end
		end
	end
end

function update_smoke()
	if (sec%interval==0) then
		smoke+=1
		if (smoke==3) then
			smoke=0
		end
	end
end

function throw_cola()
	if (cola.num>0) then
		ii=true
		ij=0
		while ii do
			ij=ij+1
			if cola.group[ij]==0 then
				ii=false
			end
		end
		cola.group[ij]={
			x=x+5+cola.r*cos(cola.theta),
			y=y+12+cola.r*sin(cola.theta),
			vx=5*(1-2*fp),
			vy=0,
			dir=0
		}
		cola.num=cola.num-1
		cola.theta=cola.theta+1/cola.mx
	end
end

function _update()
	move=false
	sec+=1
	update_smoke()
	cola.theta=cola.theta+cola.av
	
	if (sec==60) then sec=0 end

	if btn(üÖæÔ∏è) then
	 if  cola.num<cola.mx and cola.zpush==1 then
			cola.num=cola.num+1
			sfx(3,3)
		end
		cola.zpush=0
	else
			cola.zpush=1
	end
	if btn(‚ùé) then
		if cola.xpush==1 then
			throw_cola()
		end
		cola.xpush=0
	else
		cola.xpush=1
	end

	if (btn(‚¨ÜÔ∏è)) then 
		y-=1 
		move=true
		if (btn(‚¨áÔ∏è)) then
			move=false
		end
	end
	if (btn(‚¨áÔ∏è)) then 
		y+=1 
		move=true
		if (btn(‚¨ÜÔ∏è)) then
			move=false
		end
	end
	if (btn(‚û°Ô∏è)) then 
		move=true
		if (btn(‚¨ÖÔ∏è)) then
			move=false
			leg=33
			return
		end
		x+=1 
		f = false
		fp=0
		hand = 0
	end
	if (btn(‚¨ÖÔ∏è)) then 
		move=true
		x-=1
		f = true
		fp=1
		hand = 1
	end
	if (move) then leg_change()
	else leg=33
	end
	for i=1,cola.mxn do
		if not ( cola.group[i] ==0) then
			cola.group[i].x=cola.group[i].x+cola.group[i].vx
			cola.group[i].y=cola.group[i].y+cola.group[i].vy
			cola.group[i].dir=(cola.group[i].dir+1)%4
			if cola.group[i].x^2+cola.group[i].y^2>1000000 then
				cola.group[i]=0
			end
		end			  
	end
end

function bl(x)
	if x==0 then
		return false
	else
		return true
	end
end

function _draw()
	cls(3)
	--music--
	if (t==0) then
		sfx(0,0)
		sfx(1,1)
	end
	if (t==128) then
		sfx(0,0)
		sfx(2,2)
	end
	t=(t+1)%256
	
	spr(sp, x, y, 2, 2, f, false)
	spr(leg, x, y+16, 2, 2, f, false)
	spr(16,x+8-(hand*11)-smoke*hand*2+smoke,y-smoke)
	if cola.num>0 then
		for i=0,cola.num-1 do
		
		 rot=cola.theta+1/cola.mx*i
		 
			spr(
				32,
				x+5+cola.r*cos(rot),
				y+12+cola.r*sin(rot),
				1,
				1
			)
		end
	end
	for i=1,cola.mxn do
		if not ( cola.group[i] ==0) then
		
			cd_short=cola.group[i].dir
			
			spr(
				32,
				cola.group[i].x,
				cola.group[i].y,
				1,
				1,
				bl(cd_short%2),
				bl((cd_short-cd_short%2)/2)
				)
		end
	end	
	
end

